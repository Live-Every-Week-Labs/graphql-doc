{
  "name": "@graphql-docs/generator",
  "version": "0.1.0",
  "description": "GraphQL operation-first documentation generator with a React component library",
  "keywords": [
    "graphql",
    "documentation",
    "generator",
    "docusaurus",
    "mdx",
    "api-docs",
    "react",
    "components"
  ],
  "author": "Austin Zani",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./docusaurus": {
      "import": "./dist/docusaurus.js",
      "require": "./dist/docusaurus.cjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.cjs"
    },
    "./components/styles.css": "./dist/components/index.css",
    "./components/docusaurus.css": "./dist/components/docusaurus.css"
  },
  "bin": {
    "graphql-docs": "./dist/cli.js"
  },
  "files": [
    "dist",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:e2e": "vitest run src/test/e2e.test.ts",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^8.1.7",
    "@graphql-tools/load": "^8.1.6",
    "@graphql-tools/schema": "^10.0.29",
    "@graphql-tools/url-loader": "^9.0.4",
    "@inquirer/prompts": "^8.0.1",
    "ajv": "^8.12.0",
    "chalk": "^5.6.2",
    "commander": "^11.1.0",
    "cosmiconfig": "^9.0.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.0",
    "graphql": "^16.12.0",
    "graphql-config": "^5.0.0",
    "handlebars": "^4.7.8",
    "ora": "^8.2.0",
    "zod": "^3.22.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/fs-extra": "^11.0.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitest/coverage-v8": "^4.0.18",
    "axe-core": "^4.9.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "jsdom": "^27.3.0",
    "lint-staged": "^16.2.7",
    "playwright": "^1.58.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18",
    "vitest-axe": "^0.1.0"
  }
}
