{
  "$schema": "../../src/schemas/config-schema.json",
  "outputDir": "./docs/api",
  "framework": "docusaurus",
  "singlePage": false,
  "metadataDir": "../../test_schemas/ecommerce",
  "includeDeprecated": true,
  "generateSidebar": true,
  "sidebarFile": "./sidebars-api.js",
  "introDocs": [
    {
