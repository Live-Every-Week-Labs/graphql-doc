{
  "configVersion": 1,
  "outputDir": "./docs/api",
  "framework": "docusaurus",
  "metadataDir": "../../test_schemas/ecommerce",
  "llmDocs": {
    "outputDir": "./static/llm-docs",
    "baseUrl": "http://localhost:3000"
  },
  "adapters": {
    "docusaurus": {
      "singlePage": false,
      "generateSidebar": true,
      "sidebarFile": "./sidebars-api.js",
      "llmDocsBasePath": "/llm-docs",
      "introDocs": [
        {
          "source": "./docs/api-intro/overview.mdx",
          "outputPath": "intro/overview.mdx"
        },
        {
          "source": "./docs/api-intro/errors.mdx",
          "outputPath": "intro/errors.mdx"
        }
      ]
    }
  },
  "typeExpansion": {
    "maxDepth": 6,
    "defaultLevels": 0,
    "showCircularReferences": true
  }
}
